<h3>
  Create new test!
</h3>
<%= form_with model: [:admin, @test], local: true do |form| %>
  <p>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </p>
  <p>
    <%= form.label :level %>
    <%= form.select :level, ['0', '1', '2'], :prompt => 'Select level' %>
  </p>
  <p>
    <%= form.label :category %>
    <%= form.select :category_id, options_from_collection_for_select(Category.all, 'id', 'title'), :prompt => 'Select category' %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>