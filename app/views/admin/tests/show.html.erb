<div class="mt-2 row justify-content-center">
  <div class="col col-md-6">
    <h4>
      <%= @test.title%>    
    </h4>

    <table class="table table-striped table-sm mb-1 bl-1 br-1">
      <%= render @test.questions %>
    </table>
    <p class="mb-1">
      <%= link_to t('.new_question'), new_admin_test_question_path(@test) %>
    </p>
    <p class="mb-1">
      <%= link_to t('.back_to_tests'), admin_tests_path %>
    </p>
  </div>
</div>
