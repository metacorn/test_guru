<div class="mt-2 row justify-content-center">
  <div class="col col-md-3">
    <h5>
      Question
    </h5>
    <p class="mb-1">
      <%= @answer.question.body %>
    </p>
    <p class="mb-1">
      <b>Answer:</b>
      <%= @answer.body %>
    </p>
    <p class="mb-1">
      <b>Correct: </b>
      <%= @answer.correct %>
    </p>
    <%= link_to 'Back', admin_question_path(@answer.question) %>
  </div>
</div>